{
  "name": "vue-numeric-keypad",
  "version": "1.2.6",
  "description": "The virtual numeric keypad that can be used on Vue.",
  "type": "module",
  "main": "dist/vue-numeric-keypad.ssr.js",
  "browser": "dist/vue-numeric-keypad.esm.js",
  "module": "dist/vue-numeric-keypad.esm.js",
  "unpkg": "dist/vue-numeric-keypad.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "cross-env NODE_OPTIONS=--openssl-legacy-provider vue-cli-service serve dev/demo.js",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "demo": "cross-env NODE_OPTIONS=--openssl-legacy-provider vue-cli-service build dev/demo.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-replace": "^5.0.0",
    "@vitejs/plugin-vue": "^4.2.0",
    "@vue/cli-plugin-babel": "^5.0.0",
    "@vue/cli-service": "^5.0.0",
    "@vue/compiler-sfc": "^3.3.0",
    "cross-env": "^7.0.3",
    "gh-pages": "^4.0.0",
    "jsencrypt": "^3.2.1",
    "minimist": "^1.2.5",
    "rimraf": "^3.0.2",
    "rollup": "^3.20.0",
    "@rollup/plugin-terser": "^0.4.0",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^3.3.0"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chae-Sumin/vue-numeric-keypad.git"
  },
  "keywords": [
    "vue",
    "vue2",
    "vue.js",
    "random",
    "randomize",
    "virtual",
    "modal",
    "numeric",
    "number",
    "keyboard",
    "keypad",
    "encrypt",
    "encryption",
    "pseudo"
  ],
  "author": "Chae-Sumin(cotnmin)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Chae-Sumin/vue-numeric-keypad/issues"
  },
  "homepage": "https://github.com/Chae-Sumin/vue-numeric-keypad#readme"
}
